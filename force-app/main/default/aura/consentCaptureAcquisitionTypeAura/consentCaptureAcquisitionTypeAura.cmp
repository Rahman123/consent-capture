<aura:component implements="force:hasRecordId,lightning:availableForFlowScreens" description="A Container for the Consent Acquisition Capture Type Component">
    <aura:attribute name="recordId" type="String" access="Global" description="The recordId passed from the flow into the AuraComponent then to the LWC"/>
    <aura:attribute name="captureType" type="String" access="Global" description="The captureType passed from the LWC into the AuraComponent"/>

    <c:consentCaptureAcquisitionTypeSelector oncapturetypeset="{!c.handleCaptureTypeSet}"/>

</aura:component>